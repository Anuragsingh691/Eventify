<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eventify">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Eventify">
        <activity android:name=".Registrations"></activity>
        <activity android:name=".EventDetailsActivity" />
        <activity android:name=".NotesEdit" />
        <activity android:name=".SocietyDetails" />
        <activity android:name=".NewAdminLogin" />
        <activity android:name=".AdminHome" />
        <activity android:name=".AdminOtpActivity" />
        <activity android:name=".PhoneLogin" />
        <activity android:name=".AdminLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>